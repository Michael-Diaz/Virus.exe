{
  "name": "ProductiviTree",
  "description": "Rewards you with unique trees for being productive!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "/js/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "history", "tabs"],
  "action":{
    "default_popup": "/html/popup2.html",
    "default_icon": {
      "16": "/images/bonzai.png"
    }
  },
  "icons": {
    "16": "/images/bonzai.png"
  },
  "options_page": "/html/options.html"
}