{
  "name": "ProductiviTree",
  "description": "Build a forest! Zen!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "/js/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "history", "tabs"],
  "action":{
    "default_popup": "/html/initial.html",
    "default_icon": {
      "16": "/images/bonzai.png"
    }
  },
  "icons": {
    "16": "/images/bonzai.png"
  },
  "options_page": "/html/options.html",
  "web_accessible_resources": [{
    "resources": ["/images/bonzai.png"],
    "matches": [],
    "extension_ids": []
  }]
}